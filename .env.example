# .env 檔案範本

# ===============================
# DISCORD BOT CONFIGURATION
# ===============================
# 你的機器人 Token，從 Discord Developer Portal 取得
DISCORD_TOKEN=

# 你的機器人 Client ID (或稱 Application ID)，用於部署斜線指令
CLIENT_ID=


# ===============================
# POSTGRESQL DATABASE CONNECTION
# ===============================
# 資料庫主機 IP 或域名
DB_HOST=

# 資料庫連接埠，預設為 5432
DB_PORT=

# 登入資料庫的使用者名稱
DB_USER=

# 該使用者的密碼
DB_PASSWORD=

# 要連接的資料庫名稱
DB_DATABASE=